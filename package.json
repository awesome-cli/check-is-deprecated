{
  "name": "is-deprecated",
  "version": "1.0.0",
  "description": "Check if npm package is deprecated or archived",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "bin/index.js",
  "scripts": {
    "build": "rimraf bin/* && tsc -p .",
    "prettier": "prettier --write 'lib/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JB1905/is-deprecated.git"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "npm",
    "github"
  ],
  "bugs": {
    "url": "https://github.com/JB1905/is-deprecated/issues"
  },
  "homepage": "https://github.com/JB1905/is-deprecated#readme",
  "bin": {
    "is-deprecated": "bin/index.js",
    "isd": "bin/index.js"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.1.1",
    "figlet": "^1.3.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.2.0",
    "@types/node": "^13.7.7",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "lib/**/*.ts": [
      "npm run prettier"
    ]
  }
}
