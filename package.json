{
  "name": "check-is-deprecated",
  "version": "2.0.0",
  "description": "Check if npm package is deprecated or archived",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "bin/index.js",
  "scripts": {
    "build": "rimraf bin/* && tsc -p .",
    "prettier": "prettier --write 'lib/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awesome-cli/check-is-deprecated.git"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "npm",
    "github",
    "deprecated",
    "archived",
    "repository",
    "package",
    "check",
    "find"
  ],
  "bugs": {
    "url": "https://github.com/awesome-cli/check-is-deprecated/issues"
  },
  "homepage": "https://github.com/awesome-cli/check-is-deprecated#readme",
  "bin": {
    "check-is-deprecated": "bin/index.js",
    "is-deprecated": "bin/index.js",
    "cid": "bin/index.js"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^5.1.0",
    "figlet": "^1.3.0",
    "is-online": "^8.3.0",
    "log-symbols": "^3.0.0",
    "node-fetch": "^2.6.0",
    "npm-get-package-info": "^1.0.0",
    "ora": "^4.0.4"
  },
  "devDependencies": {
    "@types/figlet": "^1.2.0",
    "@types/node": "^13.13.2",
    "@types/node-fetch": "^2.5.7",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "lib/**/*.ts": [
      "npm run prettier"
    ]
  }
}
